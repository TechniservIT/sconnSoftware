        <TextBox x:Name="TbxInputUsername" HorizontalAlignment="Left" Height="23" Margin="142,41,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="350"/>
        <Label x:Name="LblDescSelGroup" Content="Group" HorizontalAlignment="Left" Margin="142,162,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.632,0" Width="94"/>
        <Label x:Name="LblDescSelPermLvl" Content="Permission" HorizontalAlignment="Left" Margin="241,162,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.632,0" Width="94"/>
